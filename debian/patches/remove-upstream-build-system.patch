-				shift
-			    fi
-			    ;;
-			-o)
-			    objsuffix="$2"
-			    shift
-			    ;;
-			
-			--*)
-			    echo "$1" | sed 's/^\-\-//' >${TMP}end
-			    endmarker="`cat ${TMP}end`"
-			    rm -f ${TMP}end
-			    if [ "$endmarker"x = x ]; then
-				endmarker="--"
-			    fi
-			    ;;
-			-v)
-			    verbose="y"
-			    ;;
-
-			-a)
-			    append="y"
-			    ;;
-
-			-cc)
-			    CC="$2"
-			    shift
-			    ;;
-
-			-*)
-			    echo "Unknown option '$1' ignored" 1>&2
-			    ;;
-			*)
-			    files="$files $1"
-			    ;;
-		    esac
-		fi
-		;;
-	esac
-    fi
-    shift
-done
-echo ' $*' >> $ARGS
-
-echo "#!/bin/sh" > $CPPCMD
-echo "exec $CC `cat $ARGS`" >> $CPPCMD
-chmod +x $CPPCMD
-rm $ARGS
-
-case "$makefile" in
-    '')
-	if [ -r makefile ]
-	then
-	    makefile=makefile
-	elif [ -r Makefile ]
-	then
-	    makefile=Makefile
-	else
-	    echo 'no makefile or Makefile found' 1>&2
-	    exit 1
-	fi
-	;;
-    -)
-	makefile=$TMPMAKEFILE
-	;;
