-SELECT
-   WHEN code=1 THEN DO
-      /* imakesvc 1 u/n dir ruledir top current */
-      instflg = WORD(all,2)
-      imakecmd = '\imake'
-      ruledir = WORD(all,4)
-      topdir = WORD(all,5)
-      currentdir = WORD(all,6)
