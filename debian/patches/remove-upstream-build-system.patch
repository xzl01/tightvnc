-	max = i - 1;
-	*p = '\0';
-	for (i = 0; i < max; i++) {
-	    if (!fncomp(&names[i], &names[i + 1]))
-		printf("8+3 clash: %s%s and %s\n",
-		       dir, names[i], names[i + 1]);
-	    free(names[i]);
-	}
-	if (names) {
-	    free(names[i]);
-	    free((char *)names);
-	}
-    }
-}
