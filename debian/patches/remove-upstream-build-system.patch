-  | sed -e 's|/[^/.][^/]*/\.\.||g' -e 's|/\.[^.][^/]*/\.\.||g' \
-    -e 's|"||g' -e 's| \./| |' \
-  | awk '{
-	if ($1 != $4  &&  $2 != "#ident" && $2 != "#pragma")
-	    {
-	    ofile = substr ($1, 1, length ($1) - 2) "'"$objsuffix"'"
-	    print ofile, $4
-	    }
-	}' \
-  | sort -u \
-  | awk '
-	    {
-	    newrec = rec " " $2
-	    if ($1 != old1)
-		{
-		old1 = $1
-		if (rec != "")
-		    print rec
-		rec = $1 ": " $2
-		}
-	    else if (length (newrec) > '"$width"')
-		{
-		print rec
-		rec = $1 ": " $2
-		}
-	    else
-		rec = newrec
-	    }
-	END \
-	    {
-	    if (rec != "")
-		print rec
-	    }' \
