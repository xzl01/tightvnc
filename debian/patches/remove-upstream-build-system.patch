-      dest = stripsuffix(WORD(all,4))
-      suffix = WORD(all,5)
-      tgt = destdir'\'dest'.'suffix
-      /* if you have no col.exe get one from 4.4BSD */
-      'groff -e -t -man -Tascii 'src'.man | col -b >'tgt
