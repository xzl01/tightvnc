-    BEGIN           {
-	csyms      = 0;		# C   language symbols in libfoo.list
-	cplusplus  = 0;		# C++ language symbols in libfoo.list
-	isyms      = 0;		# C   internal symbols in libfoo.elist
-	icplusplus = 0;		# C++ internal symbols in libfoo.elist
