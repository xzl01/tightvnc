-	}
-	if (do83) {
-	    s = (char *)malloc(strlen(dp->d_name) + 1);
-	    strcpy(s, dp->d_name);
-	    if (i >= max) {
-		max += 25;
-		if (names)
-		    names = (char **)realloc((char *)names,
-					     (max + 1) * sizeof(char *));
-		else
-		    names = (char **)malloc((max + 1) * sizeof(char *));
