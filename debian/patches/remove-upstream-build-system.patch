-	ImakeHelper 7 $(CONFIGSRC)/util/makestrs.exe /			@@\
-									@@\
-files: ProgramTargetName($(MAKESTRS)) source depends			@@\
-	RemoveFiles(files)						@@\
-	RunProgram(MAKESTRS1,options < source > dest)			@@\
-									@@\
-AllTarget(files)							@@\
-									@@\
