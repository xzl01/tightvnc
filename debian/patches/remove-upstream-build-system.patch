-int
-Strncmp(cp1, cp2, n)
-    char *cp1, *cp2;
-    int n;
-{
-    char c1, c2;
-
-    for (; --n >= 0 && *cp1 && *cp2; cp1++, cp2++) {
-	if (*cp1 != *cp2) {
-	    c1 = *cp1;
-	    c2 = *cp2;
-	    if (c1 >= 'A' && c1 <= 'Z')
-		c1 += 'a' - 'A';
-	    else if (c1 == '-')
-		c1 = '_';
-	    if (c2 >= 'A' && c2 <= 'Z')
-		c2 += 'a' - 'A';
-	    else if (c2 == '-')
-		c2 = '_';
-	    if (c1 != c2)
-		return (int)c1 - (int)c2;
-	}
-    }
-    if (n < 0)
-	return 0;
-    return (int)*cp1 - (int)*cp2;
