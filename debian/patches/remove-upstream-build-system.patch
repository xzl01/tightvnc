-	for (n = 3; n <= NF; n++)
-	    string = string " " $n;
-	print $1 ";;" string;
-	next;
-    }
-    $1 == "internal" {
-	isyms ++;
-	print $1 ";;" $2;
-	next;
-    }
-    $1 == "internalC++" {
