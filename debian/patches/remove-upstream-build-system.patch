-   END
-   WHEN code=5 THEN DO
-      /* imakesvc 5 file */
-      file = TRANSLATE(WORD(all,2),'\','/')
-      RC = SysFileDelete(file'.bak')
-      if exists(file) THEN 'REN 'file file||'.bak'
-   END
-   WHEN code=6 THEN DO
-      /* imakesvc 6 file */
